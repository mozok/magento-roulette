{
    "name": "mozok/roulette",
    "version": "1.2.0",
    "minimum-stability": "stable",
    "description": "Metapackage for roulette modules pack.",
    "replace": {
        "mozok/roulette-base": "self.version",
        "mozok/roulette-product": "self.version",
        "mozok/roulette-external-request": "self.version"
    },
    "type": "magento2-metapackage",
    "require": {
        "magento/framework": "*"
    },
    "license": [
        "Apache-2.0"
    ],
    "authors": [
        {
            "name": "Evgen Mozok",
            "email": "mozokevgen@gmail.com",
            "homepage": "https://www.facebook.com/mozok.e/",
            "role": "Developer"
        }
    ],
    "autoload": {
        "files": [
            "RouletteBase/registration.php",
            "RouletteProduct/registration.php",
            "RouletteExternalRequest/registration.php"
        ],
        "psr-4": {
            "Mozok\\RouletteBase\\": "RouletteBase",
            "Mozok\\RouletteProduct\\": "RouletteProduct",
            "Mozok\\RouletteExternalRequest\\": "RouletteExternalRequest"
        }
    }
}
